{
    "name": "ebbinghaus",
    "version": "0.0.0",
    "description": "ebbinghaus",
    "scripts": {
        "dev": "node server",
        "build": "tsc -p tsconfig.prod.json && pnpm build:client && pnpm build:server",
        "build:vercel": "tsc -p tsconfig.prod.json && vite build",
        "build:client": "vite build --outDir dist/client",
        "build:server": "vite build --ssr src/server.tsx --outDir dist/server",
        "serve": "cross-env NODE_ENV=production node server",
        "prepare": "husky install"
    },
    "author": "yytcjcy@gmail.com",
    "license": "ISC",
    "dependencies": {
        "@chakra-ui/icons": "^1.0.15",
        "@chakra-ui/react": "^1.6.7",
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11",
        "compression": "^1.7.4",
        "date-fns": "^2.23.0",
        "dexie": "^3.0.3",
        "express": "^4.17.1",
        "framer-motion": "^4",
        "graphql": "^15.5.1",
        "immer": "^9.0.5",
        "path-to-regexp": "^6.2.0",
        "react": "^18.0.0-alpha-8723e772b-20210826",
        "react-dom": "^18.0.0-alpha-8723e772b-20210826",
        "serve-static": "^1.14.1",
        "swr": "^1.0.0",
        "typescript": "^4.4.2"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.15.0",
        "@babel/eslint-plugin": "^7.14.5",
        "@ecomfe/eslint-config": "^7.2.1",
        "@types/express": "^4.17.13",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "2.24.2",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "^7.0.2",
        "lint-staged": "^11.1.2",
        "vite": "^2.5.1"
    }
