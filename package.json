{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "ebbinghaus",
    "version": "0.0.0",
    "private": true,
    "description": "ebbinghaus",
    "scripts": {
        "dev": "node server.cjs",
        "build": "tsc -p tsconfig.prod.json && pnpm build:client && pnpm build:server",
        "build:vercel": "tsc -p tsconfig.prod.json && vite build -c vite.vercel.config.ts",
        "build:client": "vite build --outDir dist/client",
        "build:server": "vite build --ssr src/server.tsx --outDir dist/server",
        "serve": "cross-env NODE_ENV=production node server.cjs",
        "prepare": "husky install"
    },
    "author": "yytcjcy@gmail.com",
    "license": "MIT",
    "dependencies": {
        "@chakra-ui/icons": "2.0.9",
        "@chakra-ui/react": "2.3.2",
        "@chakra-ui/styled-system": "2.3.1",
        "@chakra-ui/system": "2.2.9",
        "@emotion/react": "11.10.4",
        "@emotion/styled": "11.10.4",
        "compression": "1.7.4",
        "date-fns": "2.29.2",
        "dexie": "3.2.2",
        "dexie-react-hooks": "1.1.1",
        "express": "4.18.1",
        "framer-motion": "7.3.6",
        "graphql": "16.6.0",
        "path-to-regexp": "6.2.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "serve-static": "1.15.0",
        "typescript": "4.8.3"
    },
    "devDependencies": {
        "@babel/core": "7.19.1",
        "@types/express": "4.17.13",
        "@types/react": "18.0.19",
        "@types/react-dom": "18.0.6",
        "@vitejs/plugin-react": "2.1.0",
        "@yotsubacy/config": "1.7.1",
        "cross-env": "7.0.3",
        "eslint": "8.23.0",
        "husky": "8.0.1",
        "lint-staged": "13.0.3",
        "vite": "3.1.0"
    },
    "packageManager": "pnpm@7.12.1",
    "engines": {
        "node": ">=14.x"
    }
}
