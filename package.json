{
    "name": "ebbinghaus",
    "version": "0.0.0",
    "description": "ebbinghaus",
    "scripts": {
        "dev": "node server",
        "build": "tsc -p tsconfig.prod.json && pnpm build:client && pnpm build:server",
        "build:vercel": "tsc -p tsconfig.prod.json && vite build",
        "build:client": "vite build --outDir dist/client",
        "build:server": "vite build --ssr src/server.tsx --outDir dist/server",
        "serve": "cross-env NODE_ENV=production node server",
        "prepare": "husky install"
    },
    "author": "yytcjcy@gmail.com",
    "license": "MIT",
    "dependencies": {
        "@chakra-ui/icons": "^1.1.1",
        "@chakra-ui/react": "^1.7.3",
        "@emotion/react": "^11.7.0",
        "@emotion/styled": "^11.6.0",
        "compression": "^1.7.4",
        "date-fns": "^2.27.0",
        "dexie": "^3.2.0",
        "dexie-react-hooks": "^1.0.7",
        "express": "^4.17.1",
        "framer-motion": "^5.4.5",
        "graphql": "^16.1.0",
        "immer": "^9.0.7",
        "path-to-regexp": "^6.2.0",
        "react": "^18.0.0-rc.0-next-3b3daf557-20211210",
        "react-dom": "^18.0.0-rc.0-next-3b3daf557-20211210",
        "serve-static": "^1.14.1",
        "typescript": "^4.5.3"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.16.3",
        "@babel/eslint-plugin": "^7.14.5",
        "@ecomfe/eslint-config": "^7.3.0",
        "@types/express": "^4.17.13",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.4.1",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.2",
        "vite": "^2.7.1"
    },
