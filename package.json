{
    "name": "ebbinghaus",
    "version": "0.0.0",
    "private": true,
    "description": "ebbinghaus",
    "scripts": {
        "dev": "node server.cjs",
        "build": "tsc -p tsconfig.prod.json && pnpm build:client && pnpm build:server",
        "build:vercel": "tsc -p tsconfig.prod.json && vite build -c vite.vercel.config.ts",
        "build:client": "vite build --outDir dist/client",
        "build:server": "vite build --ssr src/server.tsx --outDir dist/server",
        "serve": "cross-env NODE_ENV=production node server.cjs",
        "prepare": "husky install"
    },
    "author": "yytcjcy@gmail.com",
    "license": "MIT",
    "dependencies": {
        "@chakra-ui/icons": "2.1.1",
        "@chakra-ui/react": "2.8.2",
        "@chakra-ui/styled-system": "2.9.2",
        "@chakra-ui/system": "2.6.2",
        "@emotion/react": "11.11.1",
        "@emotion/styled": "11.11.0",
        "compression": "1.7.4",
        "date-fns": "2.30.0",
        "dexie": "3.2.4",
        "dexie-react-hooks": "1.1.7",
        "express": "4.18.2",
        "framer-motion": "10.16.14",
        "graphql": "16.8.1",
        "path-to-regexp": "6.2.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "serve-static": "1.15.0",
        "typescript": "5.3.3"
    },
    "devDependencies": {
        "@babel/core": "7.23.6",
        "@types/express": "4.17.21",
        "@types/react": "18.2.43",
        "@types/react-dom": "18.2.17",
        "@vitejs/plugin-react": "2.2.0",
        "@yotsubacy/config": "1.10.4",
        "cross-env": "7.0.3",
        "eslint": "8.55.0",
        "husky": "8.0.3",
        "lint-staged": "15.2.0",
        "vite": "3.2.7"
    },
    "packageManager": "pnpm@8.12.0",
    "engines": {
        "node": ">=14.x"
    }
}
