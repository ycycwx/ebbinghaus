        "@emotion/styled": "11.8.1",
        "compression": "1.7.4",
        "date-fns": "2.28.0",
        "dexie": "3.2.1",
        "dexie-react-hooks": "1.1.1",
        "express": "4.17.3",
        "framer-motion": "6.2.10",
        "graphql": "16.3.0",
        "path-to-regexp": "6.2.0",
        "react": "18.1.0-next-fc47cb1b6-20220404",
        "react-dom": "18.1.0-next-fc47cb1b6-20220404",
        "serve-static": "1.15.0",
