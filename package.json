{
    "name": "ebbinghaus",
    "version": "0.0.0",
    "private": true,
    "description": "ebbinghaus",
    "scripts": {
        "dev": "node server",
        "build": "tsc -p tsconfig.prod.json && pnpm build:client && pnpm build:server",
        "build:vercel": "tsc -p tsconfig.prod.json && vite build",
        "build:client": "vite build --outDir dist/client",
        "build:server": "vite build --ssr src/server.tsx --outDir dist/server",
        "serve": "cross-env NODE_ENV=production node server",
        "prepare": "husky install"
    },
    "author": "yytcjcy@gmail.com",
    "license": "MIT",
    "dependencies": {
        "@chakra-ui/icons": "2.0.0-next.2",
        "@chakra-ui/react": "2.0.0-next.5",
        "@chakra-ui/system": "2.0.0-next.4",
        "@emotion/react": "11.9.0",
        "@emotion/styled": "11.8.1",
        "compression": "1.7.4",
        "date-fns": "2.28.0",
        "dexie": "3.2.2",
        "dexie-react-hooks": "1.1.1",
        "express": "4.18.1",
        "framer-motion": "6.3.3",
        "graphql": "16.5.0",
        "path-to-regexp": "6.2.1",
        "react": "18.2.0-next-7d9e17a98-20220511",
        "react-dom": "18.2.0-next-7d9e17a98-20220511",
        "serve-static": "1.15.0",
        "typescript": "4.6.4"
    },
    "devDependencies": {
        "@babel/core": "7.17.10",
        "@types/express": "4.17.13",
        "@types/react": "18.0.9",
        "@types/react-dom": "18.0.3",
        "@vitejs/plugin-react": "1.3.2",
        "@yotsubacy/config": "1.3.0",
        "cross-env": "7.0.3",
        "eslint": "8.15.0",
        "husky": "8.0.1",
        "lint-staged": "12.4.1",
        "vite": "2.9.9"
    },
    "engines": {
        "node": ">=14.x"
    }
}
